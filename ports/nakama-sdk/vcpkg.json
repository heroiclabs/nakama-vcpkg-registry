{
    "name": "nakama-sdk",
    "version": "2.6.0",
    "port-version": 3,
    "dependencies": [
        "protobuf",
        "rapidjson",
        "optional-lite",
        "vcpkg-cmake",
        "vcpkg-cmake-config"
    ],
    "features": {
        "libhttpclient-ws": {
            "description": "Websockets library that delegates to other implementations.",
            "supports": "windows | android",
            "dependencies": [
                {
                    "name": "websocketpp",
                    "default-features": false,
                    "platform": "windows"
                },
                {
                    "name": "asio",
                    "platform": "windows"
                },
                {
                    "name": "openssl",
                    "platform": "windows"
                }
            ]
        },
        "libhttpclient-http": {
            "description": "HTTP library that delegates to other implementations.",
            "supports": "windows | osx | ios | linux"
        },
        "wslay": {
            "description": "Websockets library decoupled from IO and handshakes.",
            "dependencies": [
                {
                    "name": "wslay"
                }
            ],
            "supports": "osx | ios | linux | android"
        },
        "curl-io": {
            "description": "The libcurl networking library.",
            "dependencies": [
                {
                    "name": "curl",
                    "default-features": false,
                    "features": [
                        "ssl"
                    ]
                }
            ],
            "supports": "osx | ios | linux | android"
        },
        "curl-http": {
            "description": "The libcurl networking library.",
            "dependencies": [
                {
                    "name": "curl",
                    "default-features": false,
                    "features": [
                        "ssl"
                    ]
                }
            ],
            "supports": "osx | ios | linux | android"
        },
        "libcxx": {
            "description": "An alternative C++ standard library, used by Unreal Engine.",
            "dependencies": [
                {
                    "name": "llvm",
                    "default-features": false,
                    "features": [
                        "libcxx"
                    ],
                    "platform": "linux"
                }
            ]
        },
        "logs": {
            "description": "Whether or not logs are enabled."
        },
        "cpprestsdk": {
            "description": "Maintenance-mode HTTP and Websockets library.",
            "dependencies": [
                {
                    "name": "cpprestsdk",
                    "default-features": false,
                    "features": [
                        "websockets"
                    ],
                    "platform": "android"
                }
            ],
            "supports": "android"
        }
    }
}
