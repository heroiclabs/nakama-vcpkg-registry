{
  "name": "nakama-sdk",
  "version": "2.7.1",
  "port-version": 1,
  "dependencies": [
    "optional-lite",
    "protobuf",
    "rapidjson",
    "vcpkg-cmake",
    "vcpkg-cmake-config"
  ],
  "features": {
    "cpprestsdk": {
      "description": "Maintenance-mode HTTP and Websockets library.",
      "supports": "android",
      "dependencies": [
        {
          "name": "cpprestsdk",
          "default-features": false,
          "features": [
            "websockets"
          ],
          "platform": "android"
        }
      ]
    },
    "curl-http": {
      "description": "The libcurl networking library.",
      "supports": "osx | ios | linux | android",
      "dependencies": [
        {
          "name": "curl",
          "default-features": false,
          "features": [
            "ssl"
          ]
        }
      ]
    },
    "curl-io": {
      "description": "The libcurl networking library.",
      "supports": "osx | ios | linux | android",
      "dependencies": [
        {
          "name": "curl",
          "default-features": false,
          "features": [
            "ssl"
          ]
        }
      ]
    },
    "libcxx": {
      "description": "An alternative C++ standard library, used by Unreal Engine.",
      "dependencies": [
        {
          "name": "llvm",
          "default-features": false,
          "features": [
            "libcxx"
          ],
          "platform": "linux"
        }
      ]
    },
    "libhttpclient-http": {
      "description": "HTTP library that delegates to other implementations."
    },
    "libhttpclient-ws": {
      "description": "Websockets library that delegates to other implementations.",
      "dependencies": [
        {
          "name": "asio",
          "platform": "windows"
        },
        {
          "name": "openssl",
          "platform": "windows"
        },
        {
          "name": "websocketpp",
          "default-features": false,
          "platform": "windows"
        }
      ]
    },
    "logs": {
      "description": "Whether or not logs are enabled."
    },
    "wslay": {
      "description": "Websockets library decoupled from IO and handshakes.",
      "dependencies": [
        "wslay"
      ]
    }
  }
}
